<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs.mobapde">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_chroma_square"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_chroma_square"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:largeHeap="true">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".GameActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".GameOverActivity"
            android:theme="@style/AppTheme.GameOverPopUp"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".HowToActivity"
            android:theme="@style/AppTheme.CustomPopTheme"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>